<?xml version="1.0"?>

<profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://ns.taverna.org.uk/2012/component/profile"
	xsi:schemaLocation="http://ns.taverna.org.uk/2012/component/profile ComponentProfile.xsd">

	<id>eu.scape-project.taverna.component.profile.validation</id>
	<name>Validation Component</name>
	<description>A SCAPE component for validation</description>
	<ontology id="components">https://github.com/openplanets/scape-component-profiles/raw/master/ontology/scape-component-profiles.rdf</ontology>
	<component>
		<inputPort maxDepth="1">
			<annotation>Example</annotation>
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#portType"
				class="http://scape-project.eu/pc/vocab/components#PortType">
				http://scape-project.eu/pc/vocab/components#ObjectURIPort
			</semanticAnnotation>
		</inputPort>
		<inputPort maxDepth="0" minOccurs="0" maxOccurs="unbounded">
			<annotation>Example</annotation>
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#portType"
				class="http://scape-project.eu/pc/vocab/components#MeasurePortType" />
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#acceptsMeasure"
				class="http://scape-project.eu/pw/vocab/components#Measure" />
		</inputPort>
		<inputPort maxDepth="0" minOccurs="0" maxOccurs="unbounded">
			<annotation>Example</annotation>
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#portType"
				class="http://scape-project.eu/pc/vocab/components#PortType">
				http://scape-project.eu/pc/vocab/components#ParameterPort
			</semanticAnnotation>
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#acceptsPredefinedParameter"
				class="http://scape-project.eu/pc/vocab/components#PredefinedParameter"
				minOccurs="0" maxOccurs="unbounded" />
		</inputPort>
		<outputPort maxDepth="0" minOccurs="1" maxOccurs="unbounded">
			<annotation>Example</annotation>
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#providesMeasure"
				class="http://scape-project.eu/pw/vocab/components#Measure" />
		</outputPort>
		<activity type="Tool" minOccurs="0" maxOccurs="unbounded">
			<semanticAnnotation ontology="components"
				predicate="http://scape-project.eu/pc/vocab/components#hasDependency"
				class="http://scape-project.eu/pc/vocab/components#Dependency"
				minOccurs="0" maxOccurs="unbounded" />
		</activity>
		<activity minOccurs="0" maxOccurs="unbounded" />
		<annotation>Title</annotation>
		<annotation minOccurs="0">Description</annotation>
		<annotation minOccurs="0">Author</annotation>
		<semanticAnnotation ontology="scape"
			predicate="http://scape-project.eu/pc/vocab/components#acceptsMimetype"
			class="http://scape-project.eu/pc/vocab/components#AcceptedMimetype"
			maxOccurs="unbounded" />
	</component>
</profile>